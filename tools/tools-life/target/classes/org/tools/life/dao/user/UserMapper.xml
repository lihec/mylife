<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.tools.life.dao.user.UserMapper" >
  
  <select id="getUser" parameterType="String" resultType="org.tools.life.domain.user.User">
    select usercode,password from user where usercode=#{usercode}
  </select>
  
</mapper>