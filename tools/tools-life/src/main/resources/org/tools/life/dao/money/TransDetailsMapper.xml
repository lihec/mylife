  <!-- 查询卡列表 -->
  <select id="getTransList" resultType="org.tools.life.domain.money.TransDetailsBo">
		select
			tid,toptype, type, type_name typeName,from_card fromCard, to_card toCard, money,date_format(addtime,'%Y-%m-%d %H:%i:%s') addtime,mark
			from trans_details t
			<where>
				<if test='params.type!=null and params.type!=""'> and t.type=#{params.type,jdbcType=VARCHAR}</if>
				<if test='params.fromCard!=null and params.fromCard!=""'> and t.from_card=#{params.fromCard,jdbcType=VARCHAR}</if>
				<if test='params.toCard!=null and params.toCard!=""'> and t.to_card=#{params.toCard,jdbcType=VARCHAR}</if>
				<if test='params.starttime!=null and params.starttime!=""'> and t.addtime >= str_to_date(#{params.starttime,jdbcType=VARCHAR},'%Y-%m-%d %H:%i:%s')</if>
		  		<if test='params.endtime!=null and params.endtime!=""'><![CDATA[ and t.addtime <= str_to_date(#{params.endtime,jdbcType=VARCHAR},'%Y-%m-%d %H:%i:%s') ]]></if>
			</where>
		  <if test="sortInfo!=null and sortInfo!=''">order by ${sortInfo}</if>
		  <if test="sortInfo==null or sortInfo==''">order by t.addtime</if>
  </select>
  
